package entities;

public class Student {
	public String name;
	public float trim1, trim2, trim3;
	
	public float finalGrade() {
		return trim1 + trim2 + trim3;
	}
	
	public String aproved() {
		String result = "";
		if(finalGrade() > 60) {
			result = "PASS";
		}
		else {
			result = "FAILED";
			
		}
		return result;
	}
}
