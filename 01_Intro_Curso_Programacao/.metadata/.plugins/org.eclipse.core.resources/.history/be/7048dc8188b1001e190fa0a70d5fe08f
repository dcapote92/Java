import java.util.Scanner;

public class Ex_01 {

	public static void main(String[] args) {
		final String senha = "2002";
		Scanner sc = new Scanner(System.in);
		String s = null;
		while(s != senha) {
			s = sc.next();
			System.out.println("Digite a senha: ");
			System.out.println("Senha inv√°lida!");
		}

	}

}
